<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.VOW" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#1CABB4</item>
        <item name="colorPrimaryVariant">#53BCBE</item>
        <item name="colorOnPrimary">#E2F7F8</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#1CABB4</item>
        <item name="colorSecondaryVariant">#53BCBE</item>
        <item name="colorOnSecondary">#367c83</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.VOW.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.VOW.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.VOW.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>